        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
 //När lagersaldot uppdateras för en "Consumable" så räknas snittpriset per Consumable ut och sedan uppdateras kostnaden till Consumable model.
	var model = current.model.sys_id;
	var quantity = parseInt(current.quantity, 10);
	var cost = parseInt(current.cost, 10);
	var modelCost = cost / quantity;
	//modelCost = Math.round(modelCost, 0);
	
//	gs.addInfoMessage(current.model);
//	gs.addInfoMessage(current.quantity);
//	gs.addInfoMessage(current.cost+' '+typeof(quantity));
//	gs.addInfoMessage(modelCost);
	
	var gr = new GlideRecord('cmdb_consumable_product_model');
	gr.addQuery('sys_id', model);
	gr.query();
	
	if (gr.next()){
		gr.setValue('cost', modelCost);
		gr.update();
	}
	

})(current, previous);]]></script>
