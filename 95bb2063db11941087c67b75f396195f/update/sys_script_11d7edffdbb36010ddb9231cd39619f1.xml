        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {


	//OBS!!! GRUNDFORMEL FRÅN HÅKAN A: interestRate = interest / (1-(1+interest) ^- (months/12)) / (1/(months/12)); 
	
	var interest = parseFloat(current.getValue('interest'));
	var months = parseFloat(current.getValue('months'));
	var base = parseFloat(((1+interest)).toFixed(2));
	var exp = -(months/12);
	var elevated = Math.pow(base, exp);
	//	gs.addInfoMessage("elevated =" + elevated);
	//	gs.addInfoMessage("base =" + base);
	//	gs.addInfoMessage("exp =" + exp);
	var finalRate = parseFloat(interest / (1-elevated) / (1/(months/12)));


	current.setValue('interest_rate', finalRate);
	current.update();


})(current, previous);]]></script>
