        <name>ConsumablePrice</name>
        <script><![CDATA[var ConsumablePrice = Class.create();
ConsumablePrice.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

	getPrices: function() {
		
		var model = this.getParameter('sysparm_sysid');
		
		var gr = new GlideRecord('cmdb_model');
		var answer = {};
		if (gr.get(model)) {
			answer.ConsumablePrice = gr.getValue('cost');
		}
		
		return JSON.stringify(answer);
	},
	
	
    type: 'ConsumablePrice'
});]]></script>
